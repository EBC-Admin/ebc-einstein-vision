{
  "name": "Metamind Image Identifier",
  "description": "Upload an image to metamind",
  "repository": "https://github.com/trevorscott/metamind-image-identifier",
  "logo": "http://image-identifier.herokuapp.com/static/EinsteinAtom.png",
  "keywords": ["node", "express", "metamind"],
  "image": "heroku/nodejs",
  "env":{
    "METAMIND_MODELID": {
      "description":"Optional - Your Metamind Model ID.",
      "required":false
    },
    "METAMIND_TOKEN": {
      "description":"Optional - Your Metamind JWT Token. If set, this will override the apps token regeneration functionality.",
      "required":false
    },
    "METAMIND_ACCOUNT_ID":{
      "description":"Your Metamind email address or add-on ID",
      "required":false
    },
    "METAMIND_PRIVATE_KEY":{
      "description":"Your Metamind private rsa key",
      "required":false
    }
  },
  "addons":[
    "cloudinary"
  ]
}
